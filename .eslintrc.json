{
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": "standard-with-typescript",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": ["prettier"],
	"parser": "@typescript-eslint/parser",
	"rules": {
		"no-unused-vars": "error",
		"no-const-assign":"error"
	}
}
